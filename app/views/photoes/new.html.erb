<% content_for :title do %>
	上传相片
<% end %>
<div class='box'>
<%= simple_form_for @photo, :url => photoes_path, :html => { :class => 'form-horizontal' } do |f| %>
	<%= render 'layouts/shared/error_exception', :object => @photo %>
	<%= field_set_tag '上传相片' do %>
	<div class="form-inputs">
		<%= f.input :avatar, input_html: {class: 'span6'}, as: :file, required: true %>
	</div>
	<% end %>
	<div class="form-actions">
		<%= f.button :submit, :value => '上传相片', :class => 'btn-primary' %>
	</div>
<% end %>
</div>

